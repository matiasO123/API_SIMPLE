{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "File": {
      "fileRoute": "C:/",
      "fileName": "Logs/log_",
      "fileSize": "10",
      "RollingInterval": "Day",
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Connection": "Server=.; Database=PersonDB; Trusted_Connection=true; TrustServerCertificate= true;"
  },
  "JwtSettings": {
    "Issuer": "yourIssuer",
    "Audience": "yourAudience",
    "Key": "yourSuperSecretKey1234567890abcd",
    "ExpiryInMinutes": 60
  }
}
